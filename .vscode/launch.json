    "version": "0.2.0",
    "configurations": [
        // Aliexpress scrape
        {
            "name": "Aliexpress scrape",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}${pathSeparator}manage.py",
            "args": [
                "scrape",
                "aliexpress",
                "--use-cached-orderlist",
                "--random",
                "-v",
                "3"
            ],
            "django": true,
            "justMyCode": true
        },
