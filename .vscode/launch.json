    "version": "0.2.0",
    "configurations": [
        {
            "name": "Scrape Ali",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}${pathSeparator}manage.py",
            "args": [
                "scrape",
                "aliexpress",
                "--cache-orderlist"
                "-v",
                "3"
            ],
